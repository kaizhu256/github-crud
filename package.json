{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "bin": { "github-crud": "index.js" },
    "description": "simple cli tool to PUT / GET / DELETE github files",
    "dependencies": {
        "utility2": "~2015.7.10"
    },
    "engines": { "node": ">=0.10 <=0.12" },
    "keywords": [
        "api",
        "cli", "content", "crud",
        "delete",
        "get", "git", "github",
        "put",
        "upload"
    ],
    "license": "MIT",
    "name": "github-crud",
    "os": ["darwin", "linux"],
    "repository" : {
        "type" : "git",
        "url" : "https://github.com/kaizhu256/node-github-crud.git"
    },
    "scripts": {
        "build-ci": "node_modules/.bin/utility2 shRun shReadmeBuild",
        "start": "npm_config_mode_auto_restart=1 node_modules/.bin/utility2 shRun node test.js",
        "test": "node_modules/.bin/utility2 shRun shReadmeExportPackageJson && npm_config_timeout_default=60000 node_modules/.bin/utility2 test test.js"
    },
    "version": "2015.6.3"
}